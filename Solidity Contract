pragma solidity ^0.8.20;

contract MedicineVerifier {
    mapping(bytes32 => bool) public storedHashes;

    function storeQRHash(bytes32 hash) public {
        storedHashes[hash] = true;
    }

    function verifyQRHash(bytes32 hash) public view returns (bool) {
        return storedHashes[hash];
    }
}
